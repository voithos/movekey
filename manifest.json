{
  "name": "movekey",
  "version": "1.0",
  "description": "Simple keyboard based movement.",
  "permissions": ["storage", "tabs"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["movekey.js"]
  }],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/movekey16.png",
    "32": "images/movekey32.png",
    "48": "images/movekey48.png",
    "128": "images/movekey128.png"
  },
  "manifest_version": 2
}
